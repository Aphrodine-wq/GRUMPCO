{
  "name": "grump-backend",
  "version": "2.0.0",
  "license": "MIT",
  "type": "module",
  "bin": "dist/index.js",
  "scripts": {
    "start": "node --import tsx src/index.ts",
    "start:prod": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "swc src -d dist --copy-files --config-file .swcrc && node scripts/copy-migrations.mjs",
    "build:tsc": "tsc && node scripts/copy-migrations.mjs",
    "bundle": "node scripts/esbuild-bundle.mjs",
    "bundle:windows": "node scripts/esbuild-bundle.mjs --platform=win32 --arch=x64",
    "bundle:linux": "node scripts/esbuild-bundle.mjs --platform=linux --arch=x64",
    "bundle:macos": "node scripts/esbuild-bundle.mjs --platform=darwin --arch=x64",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "test": "vitest run",
    "test:contract": "vitest run tests/contract",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:report": "vitest run --coverage && open coverage/index.html",
    "test:coverage:ci": "vitest run --coverage --reporter=json --outputFile=coverage/test-results.json",
    "load-test": "k6 run tests/performance/load-test.js",
    "load-test:dev": "k6 run tests/performance/load-test.js --vus 10 --duration 30s",
    "evals": "node --import tsx tests/evals/runEvals.ts",
    "mcp-server": "tsx src/mcp/grump-server.ts",
    "rag:index": "tsx scripts/rag-index.ts"
  },
  "dependencies": {
    "@grump/ai-core": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.57.0",
    "@opentelemetry/exporter-prometheus": "^0.57.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-node": "^0.57.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.30.0",
    "@pinecone-database/pinecone": "^6.0.0",
    "@supabase/supabase-js": "^2.91.1",
    "@types/archiver": "^7.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "archiver": "^7.0.1",
    "async-retry": "^1.3.3",
    "bullmq": "^5.34.5",
    "cls-rtracer": "^2.6.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron-parser": "^5.5.0",
    "csrf-csrf": "^4.0.3",
    "csurf": "^1.11.0",
    "discord.js": "^14.16.0",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "lru-cache": "^10.4.3",
    "multicast-dns": "^7.2.5",
    "node-cron": "^4.2.1",
    "opossum": "^5.0.1",
    "pino": "^10.3.0",
    "pino-http": "^11.0.0",
    "posthog-node": "^4.0.0",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.3.1",
    "stripe": "^17.4.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-morph": "^21.0.0",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  },
  "optionalDependencies": {
    "@sentry/node": "^8.0.0",
    "@sentry/profiling-node": "^8.0.0",
    "better-sqlite3": "^11.7.0",
    "msedge-tts": "^2.0.4",
    "playwright": "^1.58.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.3.107",
    "@types/async-retry": "^1.4.8",
    "@types/better-sqlite3": "^7.6.11",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "4.19.8",
    "@types/ioredis": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "@types/opossum": "^8.1.9",
    "@types/serve-static": "^1.15.5",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/coverage-v8": "^1.6.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "esbuild": "^0.27.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.1.0",
    "js-yaml": "^4.1.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.0"
  }
}