        {
            "name": "SHIP Workflow",
            "item": [
                {
                    "name": "Start SHIP Workflow",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"projectDescription\": \"Build a todo app with user authentication and real-time sync\",\n  \"preferences\": {\n    \"techStack\": [\"React\", \"Node.js\", \"PostgreSQL\"],\n    \"deploymentTarget\": \"docker\"\n  }\n}"
                        },
                        "url": {
                            "raw": "{{baseUrl}}/api/ship/start",
