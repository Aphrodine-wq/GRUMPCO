{
  "name": "@grump/compiler-enhanced",
  "version": "1.0.0",
  "description": "Enhanced compiler system for G-Rump with incremental compilation, watch mode, bundle analysis, HyperCompiler 40x optimization, and more",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./incremental": {
      "import": "./dist/incremental.js",
      "types": "./dist/incremental.d.ts"
    },
    "./watch": {
      "import": "./dist/watch.js",
      "types": "./dist/watch.d.ts"
    },
    "./analyzer": {
      "import": "./dist/analyzer.js",
      "types": "./dist/analyzer.d.ts"
    },
    "./dce": {
      "import": "./dist/dce.js",
      "types": "./dist/dce.d.ts"
    },
    "./parallel": {
      "import": "./dist/parallel.js",
      "types": "./dist/parallel.d.ts"
    },
    "./transforms": {
      "import": "./dist/transforms.js",
      "types": "./dist/transforms.d.ts"
    },
    "./sourcemaps": {
      "import": "./dist/sourcemaps.js",
      "types": "./dist/sourcemaps.d.ts"
    },
    "./hotreload": {
      "import": "./dist/hotreload.js",
      "types": "./dist/hotreload.d.ts"
    },
    "./hyper": {
      "import": "./dist/hyper/index.js",
      "types": "./dist/hyper/index.d.ts"
    },
    "./hyper/gpu": {
      "import": "./dist/hyper/gpu.js",
      "types": "./dist/hyper/gpu.d.ts"
    },
    "./hyper/jit": {
      "import": "./dist/hyper/jit.js",
      "types": "./dist/hyper/jit.d.ts"
    },
    "./hyper/distributed": {
      "import": "./dist/hyper/distributed.js",
      "types": "./dist/hyper/distributed.d.ts"
    },
    "./hyper/quantum": {
      "import": "./dist/hyper/quantum.js",
      "types": "./dist/hyper/quantum.d.ts"
    },
    "./hyper/probabilistic": {
      "import": "./dist/hyper/probabilistic.js",
      "types": "./dist/hyper/probabilistic.d.ts"
    },
    "./hyper/speculative": {
      "import": "./dist/hyper/speculative.js",
      "types": "./dist/hyper/speculative.d.ts"
    },
    "./hyper/mmap": {
      "import": "./dist/hyper/mmap.js",
      "types": "./dist/hyper/mmap.d.ts"
    },
    "./hyper/splitting": {
      "import": "./dist/hyper/splitting.js",
      "types": "./dist/hyper/splitting.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:hyper": "vitest run --dir src/hyper",
    "bench": "vitest bench",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "bullmq": "^5.67.3",
    "chokidar": "^5.0.0",
    "ioredis": "^5.3.0",
    "ws": "^8.18.0",
    "xxhash-addon": "^2.0.3"
  },
  "optionalDependencies": {
    "@aspect-build/dawn-node": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "@types/ws": "^8.5.13",
    "fast-check": "^4.5.3",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@grump/shared-types": "workspace:*"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "compiler",
    "intent-compiler",
    "g-rump",
    "incremental-compilation",
    "watch-mode",
    "bundle-analyzer",
    "hyper-compiler",
    "gpu-acceleration",
    "jit-compilation",
    "distributed-compilation",
    "code-splitting"
  ],
  "license": "MIT"
}
